{
  "buildCommand": "cd ../.. && bun install && cd apps/explorer && bun run build",
  "outputDirectory": "dist",
  "installCommand": "curl -fsSL https://bun.sh/install | bash && export PATH=\"$HOME/.bun/bin:$PATH\"",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}